{
  "name": "test-application",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc-alias",
    "start": "ts-node-dev -r tsconfig-paths/register -P ./tsconfig.json -T src/index.ts ",
    "start:prod": "node ./dist/index.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --passWithNoTests --coverage",
    "check:dependency": "npm run build && madge --warning -c --extensions js ./dist",
    "clean": "rm -rf node_modules .build .serverless .webpack*",
    "gen:doc": "npx openapi-typescript ./openapi-schema.yaml -o ./src/interfaces/schema.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phamtrungson/test-application.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phamtrungson/test-application/issues"
  },
  "homepage": "https://github.com/phamtrungson/test-application#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.14.2",
    "jest": "^26.6.1",
    "madge": "^7.0.0",
    "openapi-typescript": "^7.0.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "mediatr-ts": "^1.2.1",
    "nanoid": "^3.3.4",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21",
    "winston": "^3.13.0"
  }
}
